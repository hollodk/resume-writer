<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Language</th>
            <th>Level</th>
            <th>CreatedAt</th>
            <th>UpdatedAt</th>
            <th>actions</th>
        </tr>
    </thead>
    <tbody>
        {% for language in languages %}
        <tr>
            <td>{{ language.id }}</td>
            <td>{{ language.language }}</td>
            <td>{{ language.level }}</td>
            <td>{{ language.createdAt ? language.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            <td>{{ language.updatedAt ? language.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            <td>
                <a href="{{ path('language_show', {'id': language.id}) }}">show</a>
                <a href="{{ path('language_edit', {'id': language.id}) }}">edit</a>
            </td>
        </tr>
        {% else %}
        <tr>
            <td colspan="6">no records found</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
