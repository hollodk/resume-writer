<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Course</th>
            <th>Institution</th>
            <th>Start</th>
            <th>End</th>
            <th>CreatedAt</th>
            <th>UpdatedAt</th>
            <th>actions</th>
        </tr>
    </thead>
    <tbody>
        {% for course in courses %}
        <tr>
            <td>{{ course.id }}</td>
            <td>{{ course.course }}</td>
            <td>{{ course.institution }}</td>
            <td>{{ course.start }}</td>
            <td>{{ course.end }}</td>
            <td>{{ course.createdAt ? course.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            <td>{{ course.updatedAt ? course.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            <td>
                <a href="{{ path('course_show', {'id': course.id}) }}">show</a>
                <a href="{{ path('course_edit', {'id': course.id}) }}">edit</a>
            </td>
        </tr>
        {% else %}
        <tr>
            <td colspan="8">no records found</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
